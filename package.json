{
  "name": "static-server",
  "version": "1.0.0",
  "description": "vue静态化服务",
  "author": "zhangyunling",
  "scripts": {
    "dev": "cross-env NODE_ENV=local nodemon server/index.js --watch server",
    "start:dev": "cross-env NODE_ENV=dev nodemon server/index.js --watch server",
    "start:pro": "cross-env NODE_ENV=pro nodemon server/index.js --watch server",
    "lint": "eslint --fix server/"
  },
  "gitHooks": {
    "pre-commit": "npm run lint",
    "commit-msg": "node scripts/verify-commit-msg.js"
  },
  "dependencies": {},
  "devDependencies": {
    "axios": "^0.18.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "koa-bodyparser": "^4.2.1",
    "koa-route": "^3.2.0",
    "koa2": "^2.0.0-alpha.7",
    "nodemon": "^1.18.9",
    "pm2": "^5.2.0",
    "vue-server-renderer": "^2.6.10",
    "yorkie": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zhangyunling/vue-static-server.git"
  },
  "keywords": [
    "vue-static-server",
    "static-server"
  ],
  "license": "MIT",
  "engines": {
    "node": ">= 7.6.0",
    "npm": ">= 6.9.0"
  },
  "files": [
    "LICENSE",
    "README.md",
    "package.json",
    "server/"
  ]
}
